    // Tree Generator
    "tree-generator.exclude": {
        ".venv": true,
        ".git": true
    },
    "tree-generator.targets": [
        {
            "picker": {
                "label": "ASCII",
                "description": "Convert to ASCII Tree"
            },
            "beforeTree": ".\n",
            "afterTree": "",
            "indent": "│   ",
            "indentParentDirIsLast": "    ",
            "masks": {
                "root": "#1/",
                "file": {
                    "default": "├── #1",
                    "last": "└── #1"
                },
                "directory": {
                    "default": "├── #1/",
                    "last": "└── #1/"
                }
            }
        },
        {
            "picker": {
                "label": "LaTeX",
                "description": "Convert to LaTeX (DirTree)"
            },
            "beforeTree": "\\dirtree{%<br/>",
            "afterTree": "}",
            "indent": "",
            "masks": {
                "root": "  . #0 #1/ .",
                "file": {
                    "default": "  . #0 #1 ."
                },
                "directory": {
                    "default": "  . #0 #1/ ."
                }
            },
            "levelOffset": 1
        },
        {
            "picker": {
                "label": "Markdown",
                "description": "Convert to Markdown"
            },
            "beforeTree": "",
            "afterTree": "<br/>",
            "indent": "  ",
            "masks": {
                "root": "# #1<br/>",
                "file": {
                    "default": "* [#1](.#2)"
                },
                "directory": {
                    "default": "* [#1/](.#2)"
                }
            }
        }
    ],
    "files.exclude": {
        "**/__pycache__": true,
        "**/.git": false
    }

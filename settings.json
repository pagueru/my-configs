    ////// CONFIGURAÇÕES GERAIS ////////////////////////////////////////////////////////////////////

    // Catppuccin Configs
    // we try to make semantic highlighting look good
    "editor.semanticHighlighting.enabled": true,
    // prevent VSCode from modifying the terminal colors
    "terminal.integrated.minimumContrastRatio": 1,
    // make the window's titlebar use the workbench colors
    "window.titleBarStyle": "custom",
    // Configurações do VS Code
    "workbench.startupEditor": "none", // Controla a janela inicial do VS Code
    "workbench.editor.labelFormat": "short", // Nomes dos arquivos nas abas
    "window.menuBarVisibility": "classic", // Permite a menu bar na parte superior da esquerda
    "window.commandCenter": true, // Ativa a barra de pesquisa na parte superior do centro
    "breadcrumbs.enabled": true, // Caminho completo do arquivo abaixo das abas superiores
    // Configurações do Explorador de Arquivos
    "explorer.compactFolders": false, // Não compacta visualmente as pastas
    "explorer.confirmDragAndDrop": false, // Controla se é permitido arrastar e largar arquivos
    "explorer.confirmDelete": false,
    // Cofigurações do Git
    "git.autofetch": true, // Busca em intervalos por atualizações no repositório
    "git.enableSmartCommit": true, // Permite commits clicando no botão com uma mensagem padrão
    "git.path": "C:\\Program Files\\Git\\bin\\git.exe", // O caminho para o executável do Git
    // Configurações do Terminal
    "terminal.integrated.defaultProfile.windows": "Git Bash", // Define o terminal padrão
    "terminal.integrated.profiles.windows": {
        "Git Bash": {
            "source": "Git Bash",
            "args": [
                "--cd=."
            ], ///"args": ["-i"]
            "icon": "github-inverted" // "github-alt"; "github"
        }
    },
    "terminal.integrated.cwd": "${workspaceFolder}", // Caminho do workspace
    "files.encoding": "utf8",
    "terminal.integrated.inheritEnv": true,
    "terminal.integrated.fontFamily": "JetBrainsMono Nerd Font Mono",
    "terminal.integrated.gpuAcceleration": "on",
    "terminal.integrated.fontSize": 12,
    "terminal.integrated.fontWeight": "normal", // Altura da fonte do terminal
    // Configurações de Segurança
    "security.workspace.trust.banner": "never", // Remove o banner de confirmação de segurança
    "security.workspace.trust.untrustedFiles": "open", // Permite arquivos não confiáveis
    ////// CONFIGURAÇÕES E FORMATAÇÕES  DO EDITOR ///////////////////////////////////////////////////////
    // Configurações Globais do Editor
    //"editor.fontFamily": "JetBrains Mono", // A fonte do editor
    "editor.fontFamily": "JetBrainsMono Nerd Font Mono", // A fonte do terminal
    //"editor.fontFamily": "'Hack Nerd Font','Hack NF'", // A fonte do terminal
    "editor.fontSize": 12, // Tamanho da fonte do editor
    "editor.lineHeight": 1.5, // Altura da fonte do editor
    "editor.renderLineHighlight": "all", // Controla o highlith das linhas selecionadas
    "editor.cursorBlinking": "smooth", // "phase"
    "editor.cursorSmoothCaretAnimation": "on", // Habilita a animação suave para o cursor
    "editor.cursorStyle": "line", // O formato da linha do cursor
    // "editor.wordWrap": "wordWrapColumn", // Controla a quebra de linha
    // "editor.wrappingIndent": "same", // Controla o recuo da quebra de linha
    // "editor.wordWrapColumn": 100, // Largura da quebra de linha
    // "editor.rulers": [ // Largura das linhas virtuais (podem ter várias)
    //     100
    // ],
    // Configurações do Minimap
    "editor.minimap.enabled": true, // Ativa o minimap
    "editor.minimap.showSlider": "always", // Aparece um slider do tamanho da janela aberta
    "editor.minimap.size": "fill", // Expande o tamanho do minimap em relação a janela
    "editor.minimap.maxColumn": 120, // Largura do minimapa
    "editor.minimap.renderCharacters": true, // Renderiza os caracteres no minimapa
    // Configurações do Python
    "python.analysis.autoFormatStrings": true, // Adciona 'f' em strings com chaves
    "python.analysis.typeCheckingMode": "strict", // Modo de verificação de tipos
    "[python]": {
        "editor.defaultFormatter": "ms-python.black-formatter", // Formatador de arquivos
        "editor.formatOnSave": true, // Habilita a formatação ao salvar
        "editor.codeActionsOnSave": {
            "source.organizeImports": "explicit"
        },
        "editor.wordWrap": "wordWrapColumn", // Controla a quebra de linha
        "editor.wrappingIndent": "same", // Controla o recuo da quebra de linha
        "editor.wordWrapColumn": 100, // Largura da quebra de linha
        "editor.rulers": [ // Largura das linhas virtuais (podem ter várias)
            100
        ],
    },
    // Configurações do Pylint
    "pylint.args": [
        "--disable=logging-fstring-interpolation",
        "--disable=missing-module-docstring",
        "--disable=missing-function-docstring",
        "--disable=import-error"
    ],
    // Configurações do Jupyter/Notebooks
    "jupyter.interactiveWindow.textEditor.executeSelection": true,
    "notebook.formatOnCellExecution": false,
    "notebook.formatOnSave.enabled": false,
    "[notebook]": {
        "editor.defaultFormatter": "ms-python.black-formatter", // Formatador de arquivos
        "editor.formatOnSave": true, // Habilita a formatação ao salvar
        "editor.codeActionsOnSave": {
            "source.organizeImports": "explicit"
        },
        "editor.wordWrap": "wordWrapColumn", // Controla a quebra de linha
        "editor.wrappingIndent": "same", // Controla o recuo da quebra de linha
        "editor.wordWrapColumn": 100, // Largura da quebra de linha
        "editor.rulers": [ // Largura das linhas virtuais (podem ter várias)
            100
        ],
    },
    // Configurações do JSON
    "[jsonc]": {
        "editor.wordWrap": "wordWrapColumn", // Controla a quebra de linha
        "editor.wrappingIndent": "same", // Controla o recuo da quebra de linha
        "editor.wordWrapColumn": 100, // Largura da quebra de linha
        "editor.rulers": [ // Largura das linhas virtuais (podem ter várias)
            100
        ],
    },
    // Configurações do HTML
    "[html]": {
        "editor.defaultFormatter": "mohd-akram.vscode-html-format" // Formatador de arquivos
    },
    // Configurações do Markdown
    "markdownlint.config": {
        "MD041": false, // Heading levels should only increment by one level at a time
        "MD033": false // Inline HTML
    },
    "[markdown]": {
        "editor.tabSize": 2,
        "editor.wordWrap": "wordWrapColumn", // Controla a quebra de linha
        "editor.wrappingIndent": "same", // Controla o recuo da quebra de linha
        "editor.wordWrapColumn": 80, // Largura da quebra de linha
        "editor.autoClosingBrackets": "always", // Fecha os paranteses
        "editor.rulers": [ // Largura das linhas virtuais (podem ter várias)
            80
        ],
    },
    // Configurações do JavaScript
    "[javascript]": {
        "editor.tabSize": 2,
        "editor.detectIndentation": false,
    },
    // Configurações do Jinja-SQL
    "files.associations": {
        // "*.sql": "jinja-sql",
        // "*.yml": "jinja-yaml"
    },
    // Configurações SQL
    "[sql]": {
        "editor.tabSize": 4, // Controla o recuo da quebra de linha
        "editor.insertSpaces": false, // Controla o recuo da quebra de linha
        "editor.detectIndentation": false, // Controla o recuo da quebra de linha
        "files.trimTrailingWhitespace": true, // Remove os espaços em branco no final das linhas
        "files.insertFinalNewline": true, // Garante que arquivos terminem com uma linha vazia
        "editor.quickSuggestions": { // Detecção problemas e erros
            "other": true,
            "comments": false,
            "strings": true
        },
    },
    "editor.suggest.snippetsPreventQuickSuggestions": false, // Ativa sugestões de código
    "editor.wordBasedSuggestions": "allDocuments", // Realça o texto enquanto digita
    //"sqlfluff.executablePath": "C:\\Users\\rapha\\Documents\\GitHub\\dbt-core-northwind\\.venv\\Scripts\\sqlfluff.exe",
    "sqlfluff.dbtInterface.enabled": true,
    "sqlfluff.linter.run": "onType",
    "sqlfluff.experimental.format.executeInTerminal": true,
    "sqlfluff.suppressNotifications": true,
    "sqlfluff.dialect": "postgres",
    "sqlfluff.format.enabled": true,
    "sqlfluff.linter.delay": 500,
    "sqlfluff.format.languages": [
        "sql",
        "sql-bigquery",
        "jinja-sql",
        "postgres",
        "snowflake-sql",
        "dbt",
        "bigquery",
    ],
    "[jinja-sql]": {
        "editor.defaultFormatter": "dorzey.vscode-sqlfluff",
        "editor.formatOnSave": false,
    },

